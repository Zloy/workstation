alias g=git
alias s="g status"
alias d="g diff"
alias gc="g commit --verbose"
alias gb="g branch"
alias gr="g reset"
alias gl="g pull --rebase"
alias gp="g push"
alias gal="git add -A"
alias gca="gc -a"
alias gco="g checkout"
alias glg="g log"

alias doc=docker

alias b="bundle"
alias be="bundle exec"
alias bu="bundle update"

alias ls='ls --hide=*~ --color'
